@{
    string ssl = "http://";
    string hp = "";
    string language = MoreAll.Captionlanguage.SetLanguage();
    string RawUrl = Request.RawUrl.ToString();
    hp = Removelink.RemoveUrl(RawUrl);
    if (Commond.Setting("SSL").Equals("1"))
    {
        ssl = "https://";
    }
    ViewBag.Title = VS.ECommerce_MVC.App.Template.WebTitle(RawUrl);
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Html.Raw(Home.Header(ssl, Request.Url.Host, RawUrl, Commond.RequestMenu(hp), Request.RawUrl))
    <link href="/Resources/assets/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/Resources/assets/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="/Resources/assets/vendors/themify-icons/css/themify-icons.css" rel="stylesheet" />
    <link href="/Resources/assets/vendors/jvectormap/jquery-jvectormap-2.0.3.css" rel="stylesheet" />
    <link href="/Resources/assets/css/main.min.css" rel="stylesheet" />
    <script src="~/Resources/js/jquery-1.9.1.js"></script>
    <link href="~/Resources/css/Mobile.css" rel="stylesheet" />
    <style>
        .form-group {
            margin-bottom: 3px;
        }
    </style>
</head>
<body class="fixed-navbar has-animation">
    @Html.Raw(Home.Body(ssl, language))
    @Html.Partial("~/Views/Members/_header.cshtml")
    @Html.Partial("~/Views/Members/_lefmenu.cshtml")
    @RenderBody()
    @Html.Raw(Commond.Setting("Livechat"))

    <script src="/Resources/assets/vendors/popper.js/dist/umd/popper.min.js" type="text/javascript"></script>
    <script src="/Resources/assets/vendors/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/Resources/assets/vendors/metisMenu/dist/metisMenu.min.js" type="text/javascript"></script>
    <script src="/Resources/assets/vendors/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="/Resources/assets/vendors/chart.js/dist/Chart.min.js" type="text/javascript"></script>
    <script src="/Resources/assets/vendors/jvectormap/jquery-jvectormap-2.0.3.min.js" type="text/javascript"></script>
    <script src="/Resources/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js" type="text/javascript"></script>
    <script src="/Resources/assets/vendors/jvectormap/jquery-jvectormap-us-aea-en.js" type="text/javascript"></script>
    <script src="/Resources/assets/js/app.min.js" type="text/javascript"></script>
    @RenderSection("jsFooter", required: false)
    @{
        try
        {
            if (MoreAll.MoreAll.GetCookies("MembersID").ToString() != null && MoreAll.MoreAll.GetCookies("MembersID").ToString().Length > 0)
            {
            }
            else
            {
                Response.Redirect("/dang-nhap.html");
            }
        }
        catch (Exception)
        { }
    }


</body>
</html>
